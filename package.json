{
  "name": "shunkakinoki.com",
  "version": "1.2.1",
  "description": "",
  "homepage": "https://shunkakinoki.com",
  "bugs": {
    "url": "https://github.com/shunkakinoki/shunkakinoki.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shunkakinoki/shunkakinoki.com.git"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "scripts": {
    "build": "next build",
    "check": "yarn run depcheck && yarn run lint && yarn run release:check",
    "dev": "next dev",
    "eslint:check": "eslint src -c .eslintrc.json --ext .ts,.tsx --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint",
    "eslint:fix": "eslint --fix src -c .eslintrc.json --ext .ts,.tsx --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint",
    "fix": "yarn run jsonsort:fix && yarn run sort-package-json:fix && yarn run eslint:fix && yarn run prettier:fix",
    "jsonsort:check": "jsonsort *.json -a -c",
    "jsonsort:fix": "jsonsort *.json -a",
    "lint": "yarn run jsonsort:check && yarn run sort-package-json:check && yarn run eslint:check && yarn run prettier:check",
    "ncu:check": "ncu -p yarn",
    "ncu:upgrade": "ncu -u -p yarn",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "release": "semantic-release",
    "release:check": "semantic-release -d",
    "sort-package-json:check": "sort-package-json 'package.json' --check",
    "sort-package-json:fix": "sort-package-json 'package.json'",
    "start": "next start",
    "tsc": "tsc --p tsconfig.json",
    "typescript": "yarn run tsc"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook || true"
    }
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^3.13.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.1",
    "clsx": "^1.1.1",
    "next": "10.0.5",
    "next-seo": "^4.17.0",
    "next-themes": "^0.0.10",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^10.0.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "commitizen": "^4.2.3",
    "depcheck": "^1.3.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwind": "^0.2.0",
    "git-cz": "^4.7.6",
    "husky": "^4.3.8",
    "json-sort-cli": "^1.17.1",
    "npm-check-updates": "^10.2.5",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.3",
    "sort-package-json": "^1.48.1",
    "typescript": "^4.1.3"
  }
}
