{
  "name": "shunkakinoki.com",
  "version": "1.11.0",
  "description": "",
  "homepage": "https://shunkakinoki.com",
  "bugs": {
    "url": "https://github.com/shunkakinoki/shunkakinoki.com/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shunkakinoki/shunkakinoki.com.git"
  },
  "license": "MIT",
  "author": "Shun Kakinoki",
  "scripts": {
    "build": "next build",
    "build:storybook": "build-storybook -o public",
    "check": "yarn run lint && yarn run depcheck:check && yarn run release:check",
    "depcheck:check": "yarn run depcheck:root",
    "depcheck:root": "depcheck . --skip-missing=true",
    "dev": "next dev",
    "eslint:lint": "eslint . --config .eslintrc.js --ext .js,.ts,.tsx --cache --cache-location node_modules/.cache/eslint",
    "eslint:fix": "eslint --fix . --config .eslintrc.js --ext .js,.ts,.tsx --cache --cache-location node_modules/.cache/eslint",
    "fix": "concurrently 'yarn:*:fix'",
    "lint": "concurrently 'yarn:*:lint'",
    "ncu:check": "ncu --deep -p yarn",
    "ncu:upgrade": "ncu --deep -u -p yarn",
    "npm-package-json:lint": "npmPkgJsonLint .",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier:lint": "prettier --check . --ignore-unknown",
    "prettier:fix": "prettier --write . --ignore-unknown",
    "prepare": "husky install",
    "release": "yarn run semantic-release",
    "release:check": "yarn run semantic-release:check",
    "semantic-release": "semantic-release",
    "semantic-release:check": "semantic-release --dry-run",
    "storybook": "start-storybook -p 6006",
    "stylelint:check": "stylelint 'src/**/*.css'",
    "stylelint:fix": "stylelint 'src/**/*.css' --fix",
    "sort-package-json:lint": "sort-package-json 'package.json' 'apps/*/package.json' 'configurations/*/package.json' 'packages/*/package.json' --check",
    "sort-package-json:fix": "sort-package-json 'package.json' 'apps/*/package.json' 'configurations/*/package.json' 'packages/*/package.json'"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "@types/validator": "^13.6.2",
    "autoprefixer": "^10.2.6",
    "clsx": "^1.1.1",
    "gray-matter": "^4.0.3",
    "next": "11.0.1",
    "next-mdx-remote": "^2.1.3",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "next-translate": "^1.0.7",
    "notion-client": "^4.7.3",
    "notion-utils": "^4.7.3",
    "postcss": "^8.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-notion-x": "^4.7.5",
    "recoil": "^0.3.1",
    "swr": "^0.5.6",
    "tailwindcss": "^2.2.4",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@shunkakinoki/commitlint": "^1.0.0",
    "@shunkakinoki/depcheck": "^1.0.0",
    "@shunkakinoki/dev": "^1.0.0",
    "@shunkakinoki/eslint-config": "^1.0.1",
    "@shunkakinoki/husky": "^1.0.0",
    "@shunkakinoki/lint-staged": "^1.0.0",
    "@shunkakinoki/npm-check-updates": "^1.0.0",
    "@shunkakinoki/prettier": "^1.0.0",
    "@shunkakinoki/semantic-release": "^1.0.0",
    "@shunkakinoki/stylelint": "^1.0.0",
    "@shunkakinoki/tsconfig": "^1.0.1",
    "@storybook/addon-a11y": "^6.3.2",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.2",
    "@storybook/manager-webpack5": "^6.3.2",
    "@storybook/react": "^6.3.2",
    "@types/react": "^17.0.13",
    "npm-package-json-lint-config-shunkakinoki": "^1.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.42.0"
  }
}
